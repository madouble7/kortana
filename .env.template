# Kor'tana Environment Configuration
# Copy this file to .env and fill in your actual API keys
# Never commit .env files to version control

# Core AI Service API Keys
OPENAI_API_KEY=your_openai_key_here
GOOGLE_API_KEY=your_google_ai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here
XAI_API_KEY=your_xai_grok_key_here

# Vector Database Configuration
PINECONE_API_KEY=your_pinecone_key_here
PINECONE_ENV=your_pinecone_environment_here
PINECONE_INDEX=quickstart

# Security Configuration
CSRF_SECRET_KEY=your_csrf_secret_key_here
SESSION_SECRET=your_session_secret_here
KORTANA_API_KEY=kortana_api_key_change_this_in_production

# Agent Development Configuration
AGENT_LOG_LEVEL=INFO
AGENT_TRACE_ENABLED=true
AGENT_OUTPUT_DIR=agent_output
AGENT_MEMORY_THRESHOLD=20

# Model Router Configuration
DEFAULT_MODEL_ID=gpt-4o-mini-openai
FALLBACK_MODEL_ID=gemini-2.0-flash-lite
PERFORMANCE_TRACKING=true

# Development Flags
DEBUG_MODE=false
ENABLE_AGENT_TRACES=true
ENABLE_MEMORY_LOGGING=true
ENABLE_MODEL_SWITCHING=true

# Server Configuration
API_HOST=localhost
API_PORT=7777
CORS_ORIGINS=http://localhost:3000,http://localhost:3210,http://localhost:8080

# Autonomous Development Engine
ADE_ENABLED=true
ADE_SCHEDULE_ENABLED=false
ADE_MAX_TASKS_PER_CYCLE=5
ADE_PLANNING_INTERVAL_HOURS=24

# LobeChat Frontend Configuration
LOBECHAT_FRONTEND_URL=http://localhost:3210
LOBECHAT_BACKEND_URL=http://localhost:8000
