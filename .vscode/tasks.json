{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Start LobeChat Frontend",
			"type": "shell",
			"command": "npm run dev -- --port 3010",
			"group": "none",
			"isBackground": true
		},
		{
			"label": "Run Pytest",
			"type": "shell",
			"command": "c:/kortana/.kortana_config_test_env/Scripts/python.exe -m pytest tests/"
		},
		{
			"label": "Run Pytest Backslash",
			"type": "shell",
			"command": ".kortana_config_test_env\\Scripts\\python.exe -m pytest tests/"
		},
		{
			"label": "Run Pytest Direct",
			"type": "shell",
			"command": "pytest tests/"
		},
		{
			"label": "Run Core Tests",
			"type": "shell",
			"command": ".kortana_config_test_env\\Scripts\\python.exe -m pytest tests/test_model_router.py tests/test_brain.py"
		},
		{
			"label": "Run Manual Pytest Script",
			"type": "shell",
			"command": ".kortana_config_test_env\\Scripts\\python.exe run_tests_manual.py"
		},
		{
			"label": "Run Manual Pytest Relative",
			"type": "shell",
			"command": ".\\.kortana_config_test_env\\Scripts\\python.exe run_tests_manual.py"
		},
		{
			"label": "Move Model Router",
			"type": "shell",
			"command": "powershell Move-Item -Path c:\\kortana\\src\\model_router.py -Destination c:\\kortana\\src\\kortana\\model_router.py -Force"
		},
		{
			"label": "FINAL Pytest Trial",
			"type": "shell",
			"command": "c:\\kortana\\.kortana_config_test_env\\Scripts\\python.exe -m pytest tests/test_brain.py"
		},
		{
			"label": "Run Cleanup Script",
			"type": "shell",
			"command": "c:\\kortana\\.kortana_config_test_env\\Scripts\\python.exe c:\\kortana\\scripts\\root_cleanup.py"
		},
		{
			"label": "Run Brain Tests",
			"type": "shell",
			"command": "$env:PYTHONPATH=\"c:\\kortana\\src\"; c:\\kortana\\.kortana_config_test_env\\Scripts\\python.exe -m pytest c:\\kortana\\tests\\test_brain.py"
		},
		{
			"label": "Run Brain Tests V2",
			"type": "shell",
			"command": "set PYTHONPATH=c:\\kortana\\src && c:\\kortana\\.kortana_config_test_env\\Scripts\\pytest.exe c:\\kortana\\tests\\test_brain.py"
		},
		{
			"label": "Run Brain Tests V3",
			"type": "shell",
			"command": "set PYTHONPATH=c:\\kortana\\src && set PROTOCOL_BUFFERS_PYTHON_IMPLEMENTATION=python && c:\\kortana\\.kortana_config_test_env\\Scripts\\pytest.exe c:\\kortana\\tests\\test_brain.py"
		},
		{
			"label": "Unify Imports",
			"type": "shell",
			"command": "c:\\kortana\\.kortana_config_test_env\\Scripts\\python.exe c:\\kortana\\scripts\\unify_kortana_imports.py"
		},
		{
			"label": "Unify Imports Logged",
			"type": "shell",
			"command": "c:\\kortana\\.kortana_config_test_env\\Scripts\\python.exe c:\\kortana\\scripts\\unify_kortana_imports.py"
		},
		{
			"label": "Run Full Test Suite - CMD",
			"type": "shell",
			"command": "cmd.exe",
			"args": ["/c", "c:\\kortana\\run_tests.bat"],
			"presentation": {
				"reveal": "always",
				"panel": "shared"
			}
		}
	]
}